<div class= "container">
<h1>List of Restaurants</h1>
<% @restaurants.each do |restaurant| %>
<li><br><br><%= restaurant.name %>
  <%= button_to "Edit", restaurant_path(restaurant), :id => "open-contacts-dialog-btn", :class => "btn btn-outline-secondary", :method => :get %></li>
  <% end %>
</div>
<br><br><div class= "menu">
  <%= button_to "Create New", new_restaurant_path, :id => "open-contacts-dialog-btn", :class => "btn btn-outline-secondary", :method => :get %></li>
  </div>
